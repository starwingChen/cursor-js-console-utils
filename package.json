{
  "name": "cursor-js-console-utils",
  "displayName": "Cursor JS Console Utils",
  "description": "Quickly insert console.log for selected variables â€” one-click debug logging with labeled output. Supports single/multi/empty selection. Cursor & VS Code.",
  "version": "0.1.5",
  "publisher": "starwing",
  "repository": {
    "type": "git",
    "url": "https://github.com/starwingChen/cursor-js-console-utils.git"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "console",
    "utils",
    "debug",
    "javascript",
    "logging",
    "snippet",
    "cursor",
    "vscode"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "cursorJsConsoleUtils configuration",
      "properties": {
        "logFile": {
          "type": "boolean",
          "default": false,
          "description": "Whether to log file name"
        },
        "logLine": {
          "type": "boolean",
          "default": false,
          "description": "Whether to log variable line"
        }
      }
    },
    "commands": [
      {
        "command": "cursorJsConsoleUtils.insertLogStatement",
        "title": "Insert console.log for selection"
      }
    ],
    "keybindings": [
      {
        "command": "cursorJsConsoleUtils.insertLogStatement",
        "key": "shift+alt+l",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "pkg": "npx @vscode/vsce package",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/vscode": "^1.74.0",
    "typescript": "^5.0.0"
  }
}
